<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Communication/etc/communication.xsd">
    <topic name="synerise.queue.events" is_synchronous="false" request="string">
        <handler name="processSendEvent" type="Synerise\Integration\MessageQueue\Consumer\Event" method="process" />
    </topic>
    <topic name="synerise.queue.data.item" is_synchronous="false" request="\Synerise\Integration\MessageQueue\Message\Data\Item">
        <handler name="processSendUpdate" type="Synerise\Integration\MessageQueue\Consumer\Data\Item" method="process" />
    </topic>
    <topic name="synerise.queue.data.scheduler" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="ScheduleFullSynchronization" type="Synerise\Integration\MessageQueue\Consumer\Data\Scheduler" method="process" />
    </topic>
    <topic name="synerise.queue.data.customer.batch" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="processSendCustomerBatch" type="Synerise\Integration\MessageQueue\Consumer\Data\Batch\Customer" method="process" />
    </topic>
    <topic name="synerise.queue.data.customer.range" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="processSendCustomerRange" type="Synerise\Integration\MessageQueue\Consumer\Data\Range\Customer" method="process" />
    </topic>
    <topic name="synerise.queue.data.order.batch" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="processSendOrderBatch" type="Synerise\Integration\MessageQueue\Consumer\Data\Batch\Order" method="process" />
    </topic>
    <topic name="synerise.queue.data.order.range" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="processSendOrderRange" type="Synerise\Integration\MessageQueue\Consumer\Data\Range\Order" method="process" />
    </topic>
    <topic name="synerise.queue.data.product.batch" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="processSendProductBatch" type="Synerise\Integration\MessageQueue\Consumer\Data\Batch\Product" method="process" />
    </topic>
    <topic name="synerise.queue.data.product.range" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="processSendProductRange" type="Synerise\Integration\MessageQueue\Consumer\Data\Range\Product" method="process" />
    </topic>
    <topic name="synerise.queue.data.subscriber.batch" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="processSendSubscriberBatch" type="Synerise\Integration\MessageQueue\Consumer\Data\Batch\Subscriber" method="process" />
    </topic>
    <topic name="synerise.queue.data.subscriber.range" is_synchronous="false" request="Magento\AsynchronousOperations\Api\Data\OperationInterface">
        <handler name="processSendSubscriberRange" type="Synerise\Integration\MessageQueue\Consumer\Data\Range\Subscriber" method="process" />
    </topic>
</config>
