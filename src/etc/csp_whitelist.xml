<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="snrbox" type="host">*.snrbox.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="snrbox" type="host">*.snrbox.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="snrcdn" type="host">*.snrcdn.net</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>