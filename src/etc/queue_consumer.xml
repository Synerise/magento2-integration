              connection="db" />
    <consumer name="synerise.queue.data.subscriber.batch"
              queue="synerise.queue.data.subscriber.batch"
              handler="Synerise\Integration\MessageQueue\Consumer\Data\Batch\Subscriber::process"
              connection="db" />
    <consumer name="synerise.queue.data.subscriber.range"
              queue="synerise.queue.data.subscriber.range"
              handler="Synerise\Integration\MessageQueue\Consumer\Data\Range\Subscriber::process"
              connection="db" />
