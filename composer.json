{
  "name": "synerise/magento2-integration",
  "description": "Synerise Magento 2 integration module.",
  "type": "magento2-module",
  "version": "3.1.3",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2 || ^7.3",
    "anik/loguzz": "^2.2 || ^4.0",
    "mobiledetect/mobiledetectlib": "^3.74 || ^4.8"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Synerise\\Integration\\": "/",
      "Synerise\\ApiClient\\" : "lib/api-client/",
      "Synerise\\CatalogsApiClient\\" : "lib/api-catalogs/",
      "Synerise\\ItemsSearchApiClient\\" : "lib/api-items-search/",
      "Synerise\\ItemsSearchConfigApiClient\\" : "lib/api-items-search-config/"
    }
  },
  "authors": [
    {
      "name": "Synerise",
      "email": "developer@synerise.com"
    }
  ]
}
